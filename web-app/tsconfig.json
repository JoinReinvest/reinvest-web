{
  "extends": "@hookooekoo/configs-tsconfig/tsconfig-next.json",
  "compilerOptions": {
    "outDir": "./out",
    "baseUrl": "src",
    "incremental": true,
    "jsx": "preserve"
  },
  "include": [
    "./src/**/*.d.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ],
  "exclude": [
    "./out/**/*",
    "./node_modules/**/*"
  ]
}
