{
  "name": "reinvest-app",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true yarn build",
    "export": "next export",
    "build-prod": "yarn clean && yarn build && yarn export",
    "clean": "rimraf .next out",
    "style-lint": "stylelint '**/*.scss'",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix",
    "check-types": "tsc --pretty"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.2.4",
    "@tanstack/eslint-plugin-query": "4.27.0",
    "@tanstack/react-query-devtools": "4.28.0"
  },
  "dependencies": {
    "@aws-amplify/auth": "5.1.15",
    "@aws-amplify/core": "5.1.4",
    "@hookform/resolvers": "3.0.0",
    "@hookooekoo/configs-postcss": "0.1.2",
    "@hookooekoo/configs-stylelint": "0.1.4",
    "@hookooekoo/configs-tsconfig": "0.0.14",
    "@hookooekoo/eslint-config": "0.1.8",
    "@hookooekoo/ui-avatar": "0.2.3",
    "@hookooekoo/ui-checkbox": "3.1.0",
    "@hookooekoo/ui-collapsible": "0.1.5",
    "@hookooekoo/ui-dialog": "0.2.0",
    "@hookooekoo/ui-icon": "0.0.12",
    "@hookooekoo/ui-input-masked": "0.8.0",
    "@hookooekoo/ui-input-text": "0.4.0",
    "@hookooekoo/ui-link": "0.1.0",
    "@hookooekoo/ui-radio-group": "1.2.0",
    "@hookooekoo/ui-select": "0.3.5",
    "@hookooekoo/ui-stepper": "0.1.5",
    "@hookooekoo/ui-switch": "1.0.0",
    "@hookooekoo/ui-table": "0.0.15",
    "@hookooekoo/ui-typography": "0.1.5",
    "@radix-ui/react-dropdown-menu": "2.0.4",
    "@radix-ui/react-progress": "1.0.2",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tanstack/react-query": "4.28.0",
    "@types/node": "18.15.5",
    "@types/node-fetch": "2.6.2",
    "@types/react": "18.0.28",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "0.4.0",
    "classnames": "2.3.2",
    "cross-env": "7.0.3",
    "cssnano": "5.1.15",
    "dayjs": "1.11.7",
    "graphql": "16.6.0",
    "graphql-request": "5.2.0",
    "next": "13.2.4",
    "next-videos": "1.4.1",
    "node-fetch": "2.6.9",
    "postcss-scss": "4.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.7",
    "react-remove-scroll": "2.5.5",
    "recharts": "2.5.0",
    "reinvest-app-common": "git+ssh://git@github.com:JoinReinvest/reinvest-fe-common.git",
    "rimraf": "4.4.1",
    "sass": "1.59.3",
    "sharp": "^0.32.0",
    "swr": "2.1.1",
    "tailwindcss": "3.2.7",
    "tailwindcss-text-fill": "0.2.0",
    "typescript": "4.9.5",
    "zod": "3.21.4"
  },
  "eslintConfig": {
    "extends": [
      "@hookooekoo/eslint-config/next",
      "plugin:@tanstack/eslint-plugin-query/recommended"
    ],
    "plugins": [
      "@tanstack/query"
    ],
    "rules": {
      "@tanstack/query/exhaustive-deps": 0,
      "@tanstack/query/prefer-query-object-syntax": "error"
    }
  },
  "stylelint": {
    "extends": "@hookooekoo/configs-stylelint/stylelint.js"
  },
  "prettier": "@hookooekoo/eslint-config/prettier.json"
}
