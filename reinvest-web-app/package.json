{
  "name": "reinvest-app",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true yarn build",
    "export": "next export",
    "build-prod": "yarn clean && yarn build && yarn export",
    "clean": "rimraf .next out",
    "style-lint": "stylelint '**/*.scss'",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix",
    "generate": "graphql-codegen",
    "check-types": "tsc --pretty"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/client-preset": "^2.1.1",
    "@graphql-codegen/typescript": "^3.0.2",
    "@next/bundle-analyzer": "13.2.3",
    "@radix-ui/react-progress": "^1.0.2",
    "@tanstack/eslint-plugin-query": "^4.26.2",
    "@tanstack/react-query-devtools": "^4.26.1",
    "classnames": "^2.3.2",
    "cross-env": "7.0.3",
    "dayjs": "^1.11.7",
    "rimraf": "4.4.0",
    "sass": "^1.58.3",
    "tailwindcss": "3.2.7"
  },
  "dependencies": {
    "@aws-amplify/auth": "^5.1.12",
    "@aws-amplify/core": "^5.1.1",
    "@hookform/resolvers": "^2.9.11",
    "@hookooekoo/configs-postcss": "^0.1.2",
    "@hookooekoo/configs-stylelint": "^0.1.4",
    "@hookooekoo/configs-tsconfig": "^0.0.14",
    "@hookooekoo/eslint-config": "^0.1.8",
    "@hookooekoo/ui-avatar": "^0.2.3",
    "@hookooekoo/ui-checkbox": "^3.1.0",
    "@hookooekoo/ui-collapsible": "^0.1.5",
    "@hookooekoo/ui-dialog": "^0.2.0",
    "@hookooekoo/ui-icon": "^0.0.12",
    "@hookooekoo/ui-input-masked": "^0.5.0",
    "@hookooekoo/ui-input-text": "^0.4.0",
    "@hookooekoo/ui-link": "^0.1.0",
    "@hookooekoo/ui-radio-group": "^1.1.0",
    "@hookooekoo/ui-select": "^0.3.5",
    "@hookooekoo/ui-stepper": "^0.1.5",
    "@hookooekoo/ui-switch": "^1.0.0",
    "@hookooekoo/ui-table": "^0.0.15",
    "@hookooekoo/ui-typography": "^0.1.5",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tanstack/react-query": "^4.26.1",
    "@types/node": "18.15.0",
    "@types/react": "18.0.28",
    "autoprefixer": "10.4.13",
    "class-variance-authority": "^0.4.0",
    "cssnano": "^5.1.15",
    "graphql": "^16.6.0",
    "graphql-request": "^5.2.0",
    "next": "13.2.3",
    "next-videos": "1.4.1",
    "postcss-scss": "^4.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.5",
    "recharts": "^2.4.3",
    "tailwindcss-text-fill": "^0.2.0",
    "typescript": "4.9.5",
    "zod": "3.21.4"
  },
  "eslintConfig": {
    "extends": [
      "@hookooekoo/eslint-config/next",
      "plugin:@tanstack/eslint-plugin-query/recommended"
    ],
    "plugins": [
      "@tanstack/query"
    ],
    "rules": {
      "@tanstack/query/exhaustive-deps": 0,
      "@tanstack/query/prefer-query-object-syntax": "error"
    }
  },
  "stylelint": {
    "extends": "@hookooekoo/configs-stylelint/stylelint.js"
  },
  "prettier": "@hookooekoo/eslint-config/prettier.json"
}
